{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Prevent the creation of a newtab with an url, which is already opened",
  "icons": {
	  "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "dontDupTab",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "notifications",
    "webNavigation",
    "storage",
    "tabs"
  ],
  "version": "0.1.1"
}
