{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
  	"default_title": "toggle dontOpenDupTab"
  },
  "description": "Prevent opening of tabs with the same target url ",
  "icons": {
	  "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "dontOpenDupTab",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "notifications",
    "storage",
    "webNavigation",
    "tabs"
  ],
  "version": "0.2.2"
}
